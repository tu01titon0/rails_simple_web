<h1 align="center"> User management </h1>





  <table class="table">
    <thead class="thead-dark">
      <tr>
        <th scope="col">Id</th>
        <th scope="col">Name</th>
        <th scope="col">Email</th>
        <th scope="col">Action</th>
      </tr>
    </thead>
  <% @users.each do |user| %>

    <tbody>
      <tr>
        <th scope="row"><% user.id %></th>
        <td> <%=user.name %></td>
        <td><%= user.email %></td>
        <td>link</td>
      </tr>
    </tbody>
<% end %>

  </table>

